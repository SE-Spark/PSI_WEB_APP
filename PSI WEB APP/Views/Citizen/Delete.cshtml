@model PSI_WEB_APP.Models.Citizen

@{
    ViewData["Title"] = "Delete";
}

<center>
    <div class="col-sm-4 myalert alert alert-danger" role="alert">
        <h4 class="alert-heading">Delete</h4>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close" onclick="backtoList()">
            <span aria-hidden="true">&times;</span>
        </button>
        <p>Are you sure you want to delete this?</p>

        <form asp-action="Delete">
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a asp-action="Index">Back to List</a>
        </form>
    </div>
</center>
@section scripts{ 
<script type="text/javascript">
    function backtoList() {
        $.ajax({
            url:@Url.Action("")
        })
    }
</script>
}
   
